<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".acvitity.HouseListDetailedInfoActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:orientation="horizontal">
        <ImageButton
            android:id="@+id/ibtn_house_list_detailed_back"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:background="@drawable/tj_icon_back_white"/>

        <ImageButton
            android:id="@+id/ibtn_house_list_detailed_share"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            android:background="@drawable/tj_icon_share_white" />
    </RelativeLayout>
    <include layout="@layout/content_house_list_detailed_info" />
</RelativeLayout>